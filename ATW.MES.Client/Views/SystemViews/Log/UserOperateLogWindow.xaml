<Window
    x:Class="ATW.MES.Client.Views.SystemViews.Log.UserOperateLogWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:ATW.MES.Client.Views.SystemViews.Log"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="用户操作日志-原始数据"
    Width="700"
    Height="550"
    Topmost="True"
    mc:Ignorable="d">
    <Grid>

        <RichTextBox >
            <FlowDocument>
                <Paragraph>
                    <Run Text="{Binding Log}" />
                </Paragraph>
            </FlowDocument>
        </RichTextBox>
    </Grid>
</Window>
